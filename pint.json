{
    "preset": "laravel",
    "rules": {
        "array_push": true,
        "array_syntax": true,
        "backtick_to_shell_exec": true,
        "binary_operator_spaces": true,
        "blank_line_before_statement": true,
        "cast_spaces": true,
        "class_reference_name_casing": true,
        "clean_namespace": true,
        "concat_space": {
            "spacing": "one"
        }
    }
}
